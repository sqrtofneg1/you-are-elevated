{
  "name": "Note Taker App Starter",
  "version": "0.0.1",
  "description": "Take Notes on web pages!",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistest": false
  },
  "page_action": {
    "default_popup": "/popup/popup.html"
  },
  "permissions": ["tabs", "activeTab", "declarativeContent", "storage"],
  "content_scripts": [
    {
      "matches":["<all_urls>"], 
      "js": ["notes.js"],
      "css": ["notes.css"]
    }
  ]
}